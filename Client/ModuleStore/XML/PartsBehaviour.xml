<?xml version="1.0"?>
<ArrayOfCustomModuleDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <CustomModuleDefinition>
    <ModuleName>ModuleDeployablePart</ModuleName>
    <Fields>
      <!--Rotation of deployable parts, like solar panels or radiators-->
      <CustomFieldDefinition>
        <FieldName>currentRotation</FieldName>
        <IgnoreSend>false</IgnoreSend>
        <IgnoreReceive>false</IgnoreReceive>
        <IntervalCheckChangesMs>5000</IntervalCheckChangesMs>
        <IntervalApplyChangesMs>5000</IntervalApplyChangesMs>
      </CustomFieldDefinition>
      <CustomFieldDefinition>
        <FieldName>storedAnimationTime</FieldName>
        <IgnoreSend>false</IgnoreSend>
        <IgnoreReceive>false</IgnoreReceive>
        <IntervalCheckChangesMs>100</IntervalCheckChangesMs>
        <IntervalApplyChangesMs>100</IntervalApplyChangesMs>
      </CustomFieldDefinition>
    </Fields>
  </CustomModuleDefinition>
  <CustomModuleDefinition>
    <ModuleName>ModuleEngines</ModuleName>
    <Fields>
      <!--Current throttle of an engine. Probably its related with the exhaust smoke-->
      <CustomFieldDefinition>
        <FieldName>currentThrottle</FieldName>
        <IgnoreSend>false</IgnoreSend>
        <IgnoreReceive>false</IgnoreReceive>
        <IntervalCheckChangesMs>1000</IntervalCheckChangesMs>
        <IntervalApplyChangesMs>1000</IntervalApplyChangesMs>
      </CustomFieldDefinition>
      <CustomFieldDefinition>
        <FieldName>manuallyOverridden</FieldName>
        <IgnoreSend>false</IgnoreSend>
        <IgnoreReceive>false</IgnoreReceive>
        <IntervalCheckChangesMs>600000</IntervalCheckChangesMs>
        <IntervalApplyChangesMs>600000</IntervalApplyChangesMs>
      </CustomFieldDefinition>
    </Fields>
  </CustomModuleDefinition>
  <CustomModuleDefinition>
    <ModuleName>ModuleReactionWheel</ModuleName>
    <Fields>
      <!--This is just the string that says "Reaction wheels: Running" or "Reaction wheels: Idle" We don't need it at all-->
      <CustomFieldDefinition>
        <FieldName>stateString</FieldName>
        <IgnoreSend>true</IgnoreSend>
        <IgnoreReceive>true</IgnoreReceive>
      </CustomFieldDefinition>
    </Fields>
  </CustomModuleDefinition>
  <CustomModuleDefinition>
    <ModuleName>ModuleWheelSuspension</ModuleName>
    <Fields>
      <!--Position of the suspension. We don't really care about it-->
      <CustomFieldDefinition>
        <FieldName>suspensionPos</FieldName>
        <IgnoreSend>true</IgnoreSend>
        <IgnoreReceive>true</IgnoreReceive>
      </CustomFieldDefinition>
      <!--No idea-->
      <CustomFieldDefinition>
        <FieldName>autoBoost</FieldName>
        <IgnoreSend>true</IgnoreSend>
        <IgnoreReceive>true</IgnoreReceive>
      </CustomFieldDefinition>
    </Fields>
  </CustomModuleDefinition>
  <CustomModuleDefinition>
    <ModuleName>ModuleWheelBase</ModuleName>
    <Fields>
      <!--Friction of the wheels. We don't really care about it-->
      <CustomFieldDefinition>
        <FieldName>frictionMultiplier</FieldName>
        <IgnoreSend>true</IgnoreSend>
        <IgnoreReceive>true</IgnoreReceive>
      </CustomFieldDefinition>
    </Fields>
  </CustomModuleDefinition>
  <CustomModuleDefinition>
    <ModuleName>ModuleDeployableSolarPanel</ModuleName>
    <Fields>
      <!--Launch time of the panel. It afects the efficiency. An old solar panel produce less electricity-->
      <CustomFieldDefinition>
        <FieldName>launchUT</FieldName>
        <IgnoreSend>false</IgnoreSend>
        <IgnoreReceive>false</IgnoreReceive>
        <IntervalCheckChangesMs>600000</IntervalCheckChangesMs>
        <IntervalApplyChangesMs>600000</IntervalApplyChangesMs>
      </CustomFieldDefinition>
    </Fields>
  </CustomModuleDefinition>
</ArrayOfCustomModuleDefinition>