<?xml version="1.0"?>
<ModuleDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <Fields>
        <!--Rotation of deployable parts, like solar panels or radiators. No need to sync them. 
        They change WAY too often and also user doesn't controls them-->
        <FieldDefinition>
            <FieldName>currentRotation</FieldName>
            <Ignore>true</Ignore>
        </FieldDefinition>
        <FieldDefinition>
            <FieldName>storedAnimationTime</FieldName>
            <Ignore>false</Ignore>
            <ServerIgnore>false</ServerIgnore>
            <IgnoreSpectating>false</IgnoreSpectating>
            <Interval>100</Interval>
            <SetValueInModule>false</SetValueInModule>
            <CallLoad>true</CallLoad>
            <CallOnAwake>true</CallOnAwake>
            <CallOnLoad>true</CallOnLoad>
            <CallOnStart>true</CallOnStart>
        </FieldDefinition>
    </Fields>
</ModuleDefinition>
